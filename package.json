{
    "name": "rhinopython",
    "displayName": "RhinoPython",
    "description": "A Visual Studio Code extension to program python for rhino.",
    "version": "0.0.1",
    "publisher": "jingchengchen",
    "engines": {
        "vscode": "^1.22.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.CodeSender"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.CodeSender",
                "title": "CodeSender"
            }
        ],
        "keybindings":[
            {
                "command": "extension.CodeSender",
                "key": "F2"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
